            <assign variable="Fahrenheit" expression="Celsius * 9/5 + 32"/>
            <output expression="Fahrenheit &amp; &quot; degrees fahrenheit is the conversion.&quot;" newline="True"/>
            <output expression="&quot;Would you like to convert again?&quot;" newline="True"/>
            <declare name="Yes" type="Integer" array="False" size=""/>
            <declare name="No" type="Integer" array="False" size=""/>
            <input variable="Answer"/>
            <if expression="Answer = &quot;Yes&quot;">
                <then>
                    <call expression="Main"/>
                </then>
                <else>
                    <output expression="&quot;Thank you.&quot;" newline="True"/>
                </else>
            </if>
        </body>
