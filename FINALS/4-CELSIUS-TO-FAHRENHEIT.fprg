    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Celsius" type="Integer" array="False" size=""/>
            <declare name="Fahrenheit" type="Integer" array="False" size=""/>
            <declare name="Answer" type="String" array="False" size=""/>
            <output expression="&quot;Hi! We are the 'Young Programmers'. This is a program that can convert celsius into fahrenheit.&quot;" newline="True"/>
            <output expression="&quot;This is made by Lontoc, Sophia Loyde,Candelaria, Francesca Gail and Roy, Erron John.&quot;" newline="True"/>
            <output expression="&quot;Please input the degree in celsius.&quot;" newline="True"/>
